<%- include header %>
<div class="row navbar">
<h1>
  On vous livre où?
</h1>
</div>
<hr>
<div class="row">
  <form role="form" method="GET" action="paiement">
  <div class="form-group" id="locationField">
  <div class="col-sm">
      <input id="autocomplete"
      name="autoCompleteAdress"
      class="form-control"
      required="required"
      placeholder="Rechercher votre adresse"
      onFocus="geolocate()"
      type="text"/>
  </div>
  <br>
    <table id="address" class="table">
      <tbody>
      <tr>
        <td class="label label-default">Adresse de rue</td>
        <td class="slimField"><input class="form-control" id="street_number" name="street_number" disabled="true"/></td>
        <td class="wideField" colspan="2"><input class="form-control" id="route" disabled="true"/></td>
      </tr>
      <tr>
        <td class="label label-default">Ville</td>
        <td class="wideField" colspan="3"><input class="form-control" id="locality" name="city" disabled="true"/></td>
      </tr>
      <tr>
        <td class="label label-default">Etat</td>
        <td class="slimField"><input class="form-control" id="administrative_area_level_1" name="state" disabled="true"/></td>
        <td class="label">Code postal</td>
        <td class="wideField"><input class="form-control" id="postal_code" name="zipCode" disabled="true"/></td>
      </tr>
      <tr>
        <td class="label label-default">Pays</td>
        <td class="wideField" colspan="3"><input class="form-control" id="country" name="country" disabled="true"/></td>
      </tr>
    </tbody>
    </table>
    <div class="col-sm-4">
      <input id="telephone"
      class="form-control"
      name="tel"
      required="required"
      placeholder="Téléphone*"
      type="tel"/>
  </div>
  <div class="col-xs-6" id="total">
    <input id="totalPay" name="totalPay" type="hidden"/>
  </div>
</div>
<input class="btn btn-primary btn-lg" type="submit" value="Suivant">
</form>
</div>

</div>

<script src="/javascripts/jquery.min.js"></script>
<script src="/javascripts/bootstrap.min.js"></script>
<script src="/javascripts/pageMap.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB2m10W9_3IMkbSbgfRLGs6jP4ox7qcFYQ&libraries=places&callback=initAutocomplete"
async defer></script>
</body>
</html>